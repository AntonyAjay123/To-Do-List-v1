<%- include("header") -%>
  <div class="box" id="heading">
    <h1><%=kindOfDay%></h1>
  </div>

  <div class="box">
    <% for(let i=0;i<nextItemToDo.length; i++){%>
      <div class="item">
        <input type="checkbox">
        <p><%= nextItemToDo[i] %></p>
        </div>
      <%}%>
    <form class="item" action="/" method="post">
      <input type="text" name="nextItem" placeholder="New item" autocomplete="off">
      <button type="submit" name="button" value=<%= kindOfDay %>>+</button>
  </form>
  </div>

  <%- include("footer") -%>
